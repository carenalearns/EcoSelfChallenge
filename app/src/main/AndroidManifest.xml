<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.catatan">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AboutAppActivity"
            android:parentActivityName=".MainActivity"
            android:label="Tentang Aplikasi"/>
        <activity android:name=".DateActivity"
            android:parentActivityName=".MainActivity"
            android:label="Hari Peringatan Lingkungan"/>
        <activity android:name=".TipsActivity"
            android:parentActivityName=".MainActivity"
            android:label="Tips Eco Self"/>
        <activity
            android:name=".MainActivity"
            android:launchMode="singleTop">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddEditNoteActivity"
            android:parentActivityName=".MainActivity"
            android:label="Masukkan Challenge"/>
        <activity android:name=".TipsIntent"
            android:parentActivityName=".TipsActivity"
            android:label="Detail Tips"/>
        <activity
            android:name=".TipsWeb"
            android:parentActivityName=".TipsActivity"
            android:label="Video Tips"/>
    </application>

</manifest>